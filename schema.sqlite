CREATE TABLE tornado_data (
om int primary key,
Year int,
Month int,
Day int,
Date DATE,
Time TIME,
Time_Zone int,
State VARCHAR,
State_FIPS int,
State_No int,
Magnitude int,
Injuries int,
Fatalities int,
Property_Loss decimal(10,6),
Crop_Loss decimal(10,6),
Starting_Lat decimal(10,6),
Starting_Lon decimal(10,6),
Ending_Lat decimal(10,6),
Ending_Lon decimal(10,6),
Length decimal(10,6),
Width int,
fc int,
Adj_Property_Loss decimal(16,12)
)